<!-- tweets/edit -->
<h1>Edit your Tweet</h1>

<form action="/tweets/<%=@tweet.id%>" method="post">
  <input type="hidden" value="PATCH" name="_method" id="hidden">
  <input type="text" name="content" value="<%=@tweet.content%>">
  <input type="submit" id="submit" value="Edit Tweet">
</form>
<br>
<form action="/tweets/<%=@tweet.id%>" method="get">
  <input type="submit" value="Return to Tweet">
</form>
